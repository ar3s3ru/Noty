<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.danilocianfrone.noty" >

    <application
        android:allowBackup="true"
        android:name=".Noty"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".views.MainActivity" android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.TutorialActivity" android:noHistory="true" />
        <activity android:name=".views.NoteActivity" />
        <activity android:name=".views.PrefsActivity" />

    </application>

</manifest>